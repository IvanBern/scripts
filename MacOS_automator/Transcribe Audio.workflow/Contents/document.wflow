				<dict>
					<key>COMMAND_STRING</key>
					<string>cd ~/work/scripts/python/openai/
source ./venv/bin/activate
current_datetime=$(date +"%Y-%m-%d_%H%M")

for f in "$@"
do
   # Get only the filename using basename
    file_name=$(basename "$f")

   # Get only the directory path using dirname
   directory_path=$(dirname "$f")

   # Get the filename without extension
   filename_without_extension=$(basename "$f" | cut -d. -f1)
   directory_name="$current_datetime - $filename_without_extension"

   target_directory=~/Documents/Recordings/"$directory_name"
   mkdir "$target_directory"
   python3 transcribe.py -f "$f" -rf vtt &gt; "$target_directory/$file_name.vtt"   
   mv "$f" "$target_directory"

done
deactivate
